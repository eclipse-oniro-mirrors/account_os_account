import("../../../../os_account.gni")

#####################hydra-fuzz###################
import("//build/config/features.gni")
import("//build/test.gni")

##############################fuzztest##########################################
ohos_fuzztest("SetAssociatedDataFuzzTest") {
  module_out_path = fuzz_output_path

  fuzz_config_file =
      "${os_account_path}/test/fuzztest/appaccount/setassociateddata_fuzzer"
  include_dirs = []
  cflags = [
    "-g",
    "-O0",
    "-Wno-unused-variable",
    "-fno-omit-frame-pointer",
  ]
  sources = [ "setassociateddata_fuzzer.cpp" ]

  deps = [
    "${app_account_innerkits_native_path}:app_account_innerkits",
    "${common_path}:libaccount_common",
  ]

  external_deps = [
    "c_utils:utils",
    "hilog:libhilog",
    "ipc:ipc_single",
  ]

  defines = [
    "ACCOUNT_LOG_TAG = \"AppAccountFuzzTest\"",
    "LOG_DOMAIN = 0xD001B00",
  ]
}
